---
import "../styles/global.css";

const title = "Visualization Playground 2025 - Stupid Hackathon";
const description =
  "We came back — bigger than ever — by getting the entire office to take five days off from regular work(!) to create whatever we’re passionate about";
const url = "https://visplayground.punchup.world/2025";
---

<html lang="en">
  <head>
    <meta charset="utf-8" />
    <link
      rel="icon"
      type="image/svg+xml"
      href="https://punchup.world/wp-content/uploads/2019/07/cropped-symbol-mint-e1710229142879-32x32.png"
    />
    <meta name="viewport" content="width=device-width" />
    <meta name="generator" content={Astro.generator} />
    <title>{title}</title>
    <meta name="description" content={description} />
    <meta property="og:title" content={title} />
    <meta property="og:description" content={description} />
    <meta property="og:type" content="website" />
    <meta property="og:url" content={url} />
    <meta property="og:image" content={url + "/og.jpg"} />
    <script
      defer
      data-domain="visplayground.punchup.world"
      src="https://analytics.punchup.world/js/script.js"></script>
  </head>
  <body>
    <slot />
  </body>
</html>
